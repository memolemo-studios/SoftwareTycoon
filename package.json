{
  "name": "softwaretycoon",
  "version": "0.0.1-alpha.3",
  "main": "index.js",
  "license": "MPL-2.0",
  "scripts": {
    "build": "rojo build -o ./build.rbxlx",
    "compile": "rbxtsc --verbose",
    "deploy": "remodel run ./scripts/remodel/deployGame.lua",
    "extract-assets": "remodel run ./scripts/extractAssets.lua",
    "linesofcode": "node ./scripts/node/linesOfCode.js",
    "lint-eslint": "eslint \"src/**/*.*\"",
    "lint-prettier": "prettier --check \"src/**/*.*\"",
    "lint-fix": "eslint --fix \"src/**/*.*\" && prettier --write \"src/**/*.*\"",
    "lint": "yarn lint-eslint && yarn lint-prettier",
    "start": "concurrently --kill-others-on-fail \"yarn watch\" \"yarn eerve\"",
    "serve": "rojo serve",
    "test": "yarn compile && yarn test-run",
    "test-run": "run-in-roblox --place ./build.rbxlx --script ./out/server/testBootstrap.server.lua",
    "watch": "rbxtsc --verbose -w"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "^1.2.3-types.1",
    "@rbxts/types": "^1.0.542",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "concurrently": "^6.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-roblox-ts": "^0.0.32",
    "prettier": "^2.4.1",
    "rbxts-transformer-flamework": "^1.0.0-beta.7",
    "roblox-ts": "=1.2.6",
    "typescript": "=4.4.4"
  },
  "resolutions": {
    "typescript": "=4.4.4"
  },
  "dependencies": {
    "@flamework/core": "^1.0.0-beta.1",
    "@memolemo-studios/rbxts-attributes": "^1.2.2",
    "@rbxts/flipper": "^2.0.1",
    "@rbxts/goodsignal": "^0.2.0",
    "@rbxts/janitor": "^1.13.12-ts.1",
    "@rbxts/log": "^0.5.0-beta.1",
    "@rbxts/net": "^2.1.4",
    "@rbxts/profileservice": "^1.4.1",
    "@rbxts/rust-classes": "^0.11.0-beta.1",
    "@rbxts/services": "^1.2.0",
    "@rbxts/t": "^2.1.5",
    "@rbxts/testez": "^0.3.1-ts.6",
    "rbxts-transform-env": "^1.0.0-ts4.4",
    "rbxts-transformer-fs": "^1.1.0-rc.5",
    "rbxts-transformer-t": "^1.1.4"
  }
}
