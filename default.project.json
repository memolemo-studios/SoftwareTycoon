{
	"name": "roblox-ts-game",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
    "Lighting": {
      "$className": "Lighting",
      "$path": "assets/game/Lighting",
      "$properties": {
        "Technology": "ShadowMap",
				"Ambient": [0.27451, 0.27451, 0.27451],
				"ColorShift_Bottom": [0,0,0],
				"ColorShift_Top": [0,0,0],
				"OutdoorAmbient": [0.27451,0.27451,0.274511],
				"Brightness": 3,
				"EnvironmentDiffuseScale": 1,
				"EnvironmentSpecularScale": 1,
				"ClockTime": 14.5
      }
    },
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"Game": {
				"$path": "out/server"
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$path": "node_modules/@rbxts",
          "@flamework": {
            "$path": "node_modules/@flamework"
          },
          "@memolemo-studios": {
            "$path": "node_modules/@memolemo-studios"
          }
				}
			},
			"Game": {
				"$path": "out/shared"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"Game": {
					"$path": "out/client"
				}
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			},
      "$ignoreUnknownInstances": true,
      "$path": "assets/game/Workspace"
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		}
	}
}
